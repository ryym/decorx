{
  "name": "decox",
  "version": "0.0.0",
  "main": "index.js",
  "author": "ryym <ryym.64@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist-es5",
    "watch": "babel src --out-dir dist-es5 --watch",
    "build:es2015": "BABEL_ENV=es2015 babel src --out-dir dist-es2015",
    "build:all": "yarn build && yarn build:es2015",
    "lint:src": "eslint --fix src",
    "lint:test": "eslint --fix test",
    "ava": "BABEL_ENV=es2015 ava --verbose",
    "test:intg": "yarn ava -- 'test/intg/**/*.js'",
    "test:intg:w": "yarn test:intg -- --watch"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.14.0",
    "eslint-config-ryym": "^1.4.0",
    "flow-bin": "^0.38.0",
    "sinon": "^1.17.7"
  },
  "ava": {
    "source": [
      "src"
    ],
    "require": [
      "babel-register"
    ]
  }
}
