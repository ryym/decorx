{
  "name": "wramp-root",
  "private": true,
  "scripts": {
    "setup": "yarn all:install && yarn all:link",
    "all": "lerna exec -- yarn",
    "all:install": "yarn && yarn all install",
    "all:link": "yarn @link:from && yarn @link:to",
    "all:root-bin": "yarn all -- add --dev file:../../root-bin",
    "@link:to": "lerna exec yarn @link:to",
    "@link:from": "lerna exec yarn @link:from",
    "lint": "yarn lint:root-bin && yarn lint:eg && yarn all lint",
    "lint:root-bin": "eslint --fix root-bin",
    "lint:eg": "eslint --fix examples",
    "lint:pkgs": "yarn all lint",
    "pretest": "yarn setup",
    "test": "yarn all lint && yarn all test"
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "eslint-config-ryym": "^1.4.0",
    "eslint-plugin-react": "^6.9.0",
    "lerna": "2.0.0-beta.34"
  }
}
